if OpenSwitch3 == false then
   OpenSwitch3 = true
   OpenGui3.Text = "v"
FrameV1.Visible = true
FrameV2.Visible = true
FrameV3.Visible = true
FrameV4.Visible = true
FrameV5.Visible = true
else
   OpenSwitch3 = false
   OpenGui3.Text = ">"
FrameV1.Visible = false
FrameV2.Visible = false
FrameV3.Visible = false
FrameV4.Visible = false
FrameV5.Visible = false
end
end)
