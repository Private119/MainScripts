pcall(FullName)
if game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool") == game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Knife") then
   local CoreGui = game:GetService("StarterGui")

   CoreGui:SetCore("SendNotification", {
     Title = "Script",
     Text = "You Are Not The Murderer",
     Duration = 2.3,
   })
else
   if ButtonV1 == "" or ButtonV1.Text == ""..game.Players.LocalPlayer.Name or ButtonV1.Text == Ghostplayer then
      ButtonV1.Text = "Dont Put Yourself"
      wait(1)
      ButtonV1.Text = "Target Here"
   else
      if game.Players:FindFirstChild(ButtonV1.Text) then
         local TargetPlayer = unpack(GetPlayer(ButtonV1.Text)).Character.HumanoidRootPart
         TargetPlayer.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,0,-2.3)
         game.Players.LocalPlayer.Character.Humanoid:EquipTool(game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Knife"))
         wait(0.7)
         game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
      end
   end
end
end)


