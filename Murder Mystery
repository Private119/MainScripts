if LocateGun == true then
   if GunDrop then
      if LocateGunOperator == false then
         LocateGunOperator = true
         if GunDrop and not GunDrop:FindFirstChild("Gun_Located") then          
            local BillboardGui = Instance.new("BillboardGui")
            local TextLabel = Instance.new("TextLabel")

            BillboardGui.Parent = GunDrop
            BillboardGui.Name = "Gun_Locate"
            BillboardGui.AlwaysOnTop = true
            BillboardGui.LightInfluence = 1
            BillboardGui.Size = UDim2.new(0, 10, 0, 10)
            BillboardGui.StudsOffset = Vector3.new(0, 0, 0)

            TextLabel.Parent = BillboardGui
            TextLabel.BackgroundColor3 = Color3.new(10, 15, 0)
            TextLabel.BackgroundTransparency = 0
            TextLabel.Size = UDim2.new(1, 0, 1, 0)
            TextLabel.Text = ""
            TextLabel.TextColor3 = Color3.new(1, 0, 0)
            TextLabel.TextScaled = true
         end
      end
   else
      LocateGunOperator = false
   end
end
end)
