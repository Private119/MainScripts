---------Script Filtering
MainFrame2.Text = "Murder Mystery"



--Auto Get Dun Drop Source Code
local function AutoGetGunActive()
GunFound = false
getgenv().AutoGetGun = true
while getgenv().AutoGetGun == true do
spawn(function()
if GunFound == false and GunDrop then
   GunFound = true
   local old = game.Players.LocalPlayer.Character:getChildren()
   for i=1,#old do
     if old[i].Name == "HumanoidRootPart" then
        LP = old[i].CFrame
     end
   end
   wait()
   game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = GunDrop.CFrame * CFrame.new(0,2,0)
   wait(0.15)
   game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = LP
   wait(2)
   if GunDrop then
      GunFound = false
   end
end
if GunDrop == nil then
   GunFound = false
end
if not game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") or game.Players.LocalPlayer.Character.Humanoid.Health == 0 or game.Players.LocalPlayer.Character.Humanoid.Health <= 1 then
  GunFound = false
end
end)
wait(0.1)
end
end
--


Switch5 = false
Buttonv1.Text = "Auto Get Gun Drop"
Buttonv1.MouseButton1Click:connect(function()
if Switch5 == false then
   Switch5 = true
   Buttonv1.Text = "UnAuto Get Gun Drop"
   AutoGetGunActive()
else
   Switch5 = false
   Buttonv1.Text = "Auto Get Gun Drop"
   getgenv().AutoGetGun = false
end
end)





--Auto Avoid Murder Operator
local part = Instance.new("Part")
part.Name = "CameraPart"
part.Color = Color3.new(0,0,0)
part.Material = Enum.Material.Plastic
part.Transparency = 1
part.Position = Vector3.new(0,10000,0)
part.Size = Vector3.new(1,0.5,1)
part.CastShadow = true
part.Anchored = true
part.CanCollide = false
part.Parent = workspace

local MURDERER = nil
local AvoidMurder = false
local AvoidOperator = false

local function AvoidReset()
 MURDERER = nil
 if AvoidOperator == true then
   game.Workspace.CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character.Humanoid
   workspace:FindFirstChild("CameraPart").CFrame = CFrame.new(0,200000,0)
   game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = AvoidPs
   AvoidOperator = false
 end
end

game:GetService('RunService').RenderStepped:connect(function()
if AvoidMurder == true then
   if MURDERER == nil then
      roles = ReplicatedStorage:FindFirstChild("GetPlayerData", true):InvokeServer()
      for i, v in pairs(roles) do
        if v.Role == "Murderer" then
	       Murder = i
        end
      end

      for _, v in pairs(game.Players:GetChildren()) do
        if v ~= game.Players.LocalPlayer and v.Character then
	       if v.Name == Murder and IsAlive(v) then
	          MURDERER = v
	       end
        end
      end
   else
      if MURDERER.Name ~= game.Players.LocalPlayer.Name then 
         if MURDERER.Character:FindFirstChild("Knife") or MURDERER.Backpack:FindFirstChild("Knife") then else
            AvoidReset()
         end
         if MURDERER.Character.Humanoid.Health <= 0 then
            AvoidReset()
         end
         wait(0.1)
         if not game.Players:FindFirstChild(MURDERER.Name) then
            AvoidReset()
         end
         if AvoidOperator == false and math.floor((MURDERER.Character.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude) <= 20 or AvoidOperator == false and math.floor((MURDERER.Character.HumanoidRootPart.Position - workspace:FindFirstChild("CameraPart").Position).magnitude) <= 20 then
            AvoidOperator = true
            local old = game.Players.LocalPlayer.Character:getChildren() 
            for i=1,#old do 
              if old[i].Name == "HumanoidRootPart" then 
                 AvoidPs = old[i].CFrame 
              end 
            end
            workspace:FindFirstChild("CameraPart").CFrame = game.Players.LocalPlayer.Character.Head.CFrame
            ---Teleport To Lobby
            for i,v in pairs(workspace:GetDescendants()) do
              if v:IsA("BasePart") and v.Parent.Name == "Spawns" and v.Parent.Parent.Name == "Lobby" then
                 game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame * CFrame.new(0,3,0)
              end
            end
            ---
            game.Workspace.CurrentCamera.CameraSubject = workspace:FindFirstChild("CameraPart")
         end
         if AvoidOperator == true and math.floor((MURDERER.Character.HumanoidRootPart.Position - workspace:FindFirstChild("CameraPart").Position).magnitude) >= 20 then
            AvoidOperator = false
            game.Workspace.CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character.Humanoid
            workspace:FindFirstChild("CameraPart").CFrame = CFrame.new(0,200000,0)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = AvoidPs
         end
      end
   end
end
end)
--

Switch6 = false
Buttonv2.Text = "Auto Avoid Murder"
Buttonv2.MouseButton1Click:connect(function()
if Switch6 == false then
   Switch6 = true
   Buttonv2.Text = "UnAuto Avoid Murder"
   AvoidMurder = true
   MURDERER = nil
   AvoidOperator = false
else
   Switch6 = false
   Buttonv2.Text = "Auto Avoid Murder"
   if AvoidOperator == true and MURDERER.Character:FindFirstChild("HumanoidRootPart") then
      game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = AvoidPs
   end
   MURDERER = nil
   AvoidPs = nil
   AvoidMurder = false
   AvoidOperator = false
   workspace:FindFirstChild("CameraPart").CFrame = CFrame.new(0,200000,0)
   game.Workspace.CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character.Humanoid
end
end)




--Game Countdown Source Code
local TimerGui = Instance.new("ScreenGui")
local TimerFrame = Instance.new("Frame")
local Timer = Instance.new("TextButton")

TimerGui.Parent = game.CoreGui

Timer.Parent = TimerGui
Timer.BackgroundColor3 = Color3.new(0,0,0)
Timer.BackgroundTransparency = 1
Timer.Position = UDim2.new(0.5, 0, 0.05, 0)
Timer.TextColor3 = Color3.new(1,1,1)
Timer.Size = UDim2.new(0,0.9,0)
Timer.Text = "0:00"
Timer.TextScaled = false
Timer.TextSize = 16
Timer.TextWrapped = false
Timer.Draggable = false
Timer.Visible = false

local function resetcountdown()
Countdown = false
CountdownOperator = false
CountdownFoundMurder = false
MinTime = 0
SecTime = 0
Murderer = nil
Timer.Text = "0:00"
end

Murderer = nil
StartGame = false
TimerCountdown = false
Countdown = false
CountdownFoundMurder = false
CountdownOperator = false
MinTime = 3
SecTime = 0
game:GetService('RunService').Heartbeat:connect(function()
if StartGame == true then
   if Murderer == nil then
      Timer.Text = "0:00"
      StartGame = false
      TimerCountdown = true
   else
      Timer.Text = "Wait Next Round To Work"
      if Murderer.Character:FindFirstChild("Knife") or Murderer.Backpack:FindFirstChild("Knife") then else
         TimerCountdown = true
         StartGame = false
         Timer.Text = "0:00"
      end
      if game.Players:FindFirstChild()..Murderer.Name then else
         TimerCountdown = true
         StartGame = false
         Timer.Text = "0:00"
      end
      if Murderer.Character:FindFirstChild("HumanoidRootPart") then else
         TimerCountdown = true
         StartGame = false
         Timer.Text = "0:00"
      end
      if Countdown == true and Murderer.Character.Humanoid.Health <= 0 then
         TimerCountdown = true
         StartGame = false
         Timer.Text = "0:00"
      end
      wait(0.1)
      if not game.Players:FindFirstChild(Murderer.Name) then
         TimerCountdown = true
         StartGame = false
         Timer.Text = "0:00"
      end
   end
end

if TimerCountdown == true then
   if Murderer == nil then
      if CountdownFoundMurder == false then
         for i,v in pairs(game.Players:GetPlayers()) do
           if v.Backpack:FindFirstChild("Knife") and v.Character.Humanoid.Health >= 1 then
              CountdownFoundMurder = true
              Countdown = true
              MinTime = 3
              SecTime = 0
              Murderer = v
           end
         end
      end
   end
   if Countdown == true and CountdownOperator == false then
      CountdownOperator = true
      wait(0.97)
      if SecTime <= -1 then
         SecTime = 59
         MinTime -= 1
      end
      if SecTime <= 9 then
         Timer.Text = MinTime..":0"..SecTime
      else
         Timer.Text = MinTime..":"..SecTime
      end
      if MinTime <= 0 and SecTime <= 0 then
         resetcountdown()
      end
      SecTime -= 1
      CountdownOperator = false
   end
   if Murderer ~= nil then
      if Murderer.Character:FindFirstChild("Knife") or Murderer.Backpack:FindFirstChild("Knife") then else
         resetcountdown()
      end
      if Murderer.Character.Humanoid.Health <= 0 then
         resetcountdown()
      end
      wait(0.1)
      if not game.Players:FindFirstChild(Murderer.Name) then
         resetcountdown()
      end
   end
end
end)
--


Switch7 = false
Buttonv3.Text = "Game Countdown"
Buttonv3.MouseButton1Click:connect(function()
if Switch7 == false then
   Switch7 = true
   Timer.Visible = true
   Murderer = nil
   StartGame = true
   TimerCountdown = false
   Countdown = false
   CountdownFoundMurder = false
   CountdownOperator = false
   MinTime = 3
   SecTime = 0
   for i,v in pairs(game.Players:GetPlayers()) do
     if v.Character:FindFirstChild("Knife") or v.Backpack:FindFirstChild("Knife") then
        Murderer = v
     end
   end
else
   Switch7 = false
   Timer.Visible = false
   Murderer = nil
   StartGame = false
   TimerCountdown = false
   Countdown = false
   CountdownFoundMurder = false
   CountdownOperator = false
   MinTime = 3
   SecTime = 0
end
end)






