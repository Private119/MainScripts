OpenGui3.Size = UDim2.new(0.2,0,1)
OpenGui3.Font = Enum.Font.SourceSansLight
OpenGui3.FontSize = Enum.FontSize.Size14
OpenGui3.Text = ">"
OpenGui3.TextScaled = true
OpenGui3.TextSize = 8
OpenGui3.TextWrapped = true

OpenSwitch3 = false
