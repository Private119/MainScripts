Cancel.MouseButton1Click:Connect(function()
UIScriptScrolling.Visible = false
Cancel.Visible = false
end)

EmoteButton1.MouseButton1Click:Connect(function()
ButtonChoosed = "EmoteButton1"
ChooseEmote()
end)

